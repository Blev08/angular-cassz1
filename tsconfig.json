{
  "compileOnSave": false,
  "compilerOptions":{
    "baseUrl": "src",
    "paths":{
      "@app/*": ["app/*"],
      "@services/*": ["app/services/*"],
      "@components/*": ["app/components/*"],
      "@environments/*": ["environments/*"],
      "@assets/*": ["assets/*"]
    },
    "outDir":"./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "es5",
    "typeRoots":[
      "node_modules/@types"
    ],
    "lib":[
      "es2017",
      "dom"
    ]
  }
}